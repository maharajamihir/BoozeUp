{
  "expo": {
    "name": "BoozeUp",
    "slug": "BoozeUp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "BoozeUp - Buy and Sell Booze convienently and easily using an App. No hassle to reach the stores before they close or to drive far too get booze in the evening. Locate Booze in your Area and keep the party running!",
    "githubUrl": "https://github.com/maharajamihir/BoozeUp/"
  }
}
